<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="project.mall.goods.model.SimpleNotification" table="T_SIMPLE_NOTIFICATION">
        <id name="id" type="java.lang.String">
            <column name="UUID" />
            <generator class="uuid.hex" />
        </id>

        <property name="fromId" type="java.lang.String">
            <column name="FROM_ID" />
        </property>

        <property name="aimId" type="java.lang.String">
            <column name="AIM_ID" />
        </property>

        <property name="tipCode" type="java.lang.String">
            <column name="TIP_CODE" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="TITLE" />
        </property>
        <property name="state" type="java.lang.Integer">
            <column name="STATE" />
        </property>

        <property name="content" type="java.lang.String">
            <column name="CONTENT" />
        </property>

        <property name="createTime" type="java.time.LocalDateTime">
            <column name="CREATE_TIME" />
        </property>

    </class>
</hibernate-mapping>
