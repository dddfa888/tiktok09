{"ast":null,"code":"import \"vant/es/overlay/style\";\nimport _Overlay from \"vant/es/overlay\";\nimport { mapMutations, mapGetters } from \"vuex\";\nimport { apiHeartbeat, apiGetMaxBuyNum, apiGetCustomerService } from '@/API/common';\nimport { apiApplyLoan } from \"./API/loan\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      show: false,\n      timer: \"\",\n      itemData: this.$store.state.multiItem[process.env.VUE_APP_ITEM_NAME],\n      itemName: process.env.VUE_APP_ITEM_NAME,\n      mainColor: process.env.VUE_APP_MAIN_COLOR\n    };\n  },\n  components: {\n    [_Overlay.name]: _Overlay\n  },\n  created() {\n    if (this.$route.query.lang) {\n      if (this.$route.query.lang == \"cn\") {\n        this.handleSetLang(\"cn\");\n      } else {\n        this.handleSetLang(\"en\");\n      }\n    }\n    if (window.plus) {\n      this.plusReady();\n    } else {\n      document.addEventListener(\"plusready\", this.plusReady, false);\n    }\n    let link = document.head.querySelector(\"link\");\n    link.href = require(`@/assets/${this.itemName}/favicon.png`);\n    document.title = this.itemName;\n    // 刷新事件\n    window.addEventListener(\"beforeunload\", () => {\n      sessionStorage.removeItem(\"scrollTopCount\");\n      // sessionStorage.removeItem(\"showLoading\")\n    });\n    if (localStorage.getItem('lang') == 'en-US') localStorage.setItem('lang', 'en');\n    setInterval(() => {\n      let token = localStorage.getItem(\"token\");\n      token && apiHeartbeat();\n    }, 1000 * 60 * 5);\n    this.getMaxBuyNum();\n    this.getCustomerService();\n  },\n  methods: {\n    plusReady() {\n      // H5 plus事件处理\n      // eslint-disable-next-line no-undef\n      plus.screen.lockOrientation(\"portrait-primary\");\n\n      // 设置系统状态栏背景为红色\n      // eslint-disable-next-line no-undef\n      // var type = plus.os.name;\n      // if (type === \"Android\" || type === \"iOS\") {\n      //     switch (this.$route.path) {\n      //         case \"/me\":\n      //             // eslint-disable-next-line no-undef\n      //             plus.navigator.setStatusBarStyle(\"light\");\n      //             // eslint-disable-next-line no-undef\n      //             plus.navigator.setStatusBarBackground(this.mainColor);\n      //             break;\n      //         default:\n      //             // eslint-disable-next-line no-undef\n      //             plus.navigator.setStatusBarStyle(\"dark\");\n      //             // eslint-disable-next-line no-undef\n      //             plus.navigator.setStatusBarBackground(\"#ffffff\");\n      //     }\n      // }\n    },\n    // 获取商品最大购买数\n    async getMaxBuyNum() {\n      let {\n        mall_max_goods_number_in_order\n      } = await apiGetMaxBuyNum({\n        code: 'mall_max_goods_number_in_order'\n      });\n      localStorage.setItem('maxBuyNum', mall_max_goods_number_in_order);\n    },\n    // 获取客服地址\n    async getCustomerService() {\n      let {\n        customer_service_url\n      } = await apiGetCustomerService('?code=customer_service_url');\n      if (customer_service_url) {\n        const {\n          multiItem\n        } = this.$store.state;\n        const itemName = this.itemName;\n\n        // 如果对应的 item 不存在，先初始化为一个对象\n        if (!multiItem[itemName]) {\n          // ⚠️ 直接修改 Vuex 的 state 是不推荐的，这里只是防止报错\n          // 正确做法见下方 Vuex 建议\n          this.$set(multiItem, itemName, {});\n        }\n\n        // multiItem[itemName].customer_service = customer_service_url;\n      }\n    },\n    handleSetLang(lang) {\n      // 设置i18n.locale 组件库会按照上面的配置使用对应的文案文件\n      this.$i18n.locale = lang;\n      // 提交mutations\n      this.setLanguage(lang);\n    }\n  },\n  watch: {\n    \"$store.state\": function (newVal) {\n      if ($store.state.qiangdan.qiangdan_status) {\n        this.show = true;\n      }\n    },\n    $route: function (to, from) {\n      if (to.name !== 'home') {\n        document.body.scrollTop = 0;\n        document.documentElement.scrollTop = 0;\n      } else {\n        setTimeout(() => {\n          window.scrollTo({\n            top: sessionStorage.getItem(\"scrollTopCount\"),\n            behavior: \"smooth\"\n          });\n        }, 500);\n      }\n    }\n  },\n  beforeDestroy() {\n    clearInterval(this.timer);\n  }\n};","map":{"version":3,"names":["mapMutations","mapGetters","apiHeartbeat","apiGetMaxBuyNum","apiGetCustomerService","apiApplyLoan","name","data","show","timer","itemData","$store","state","multiItem","process","env","VUE_APP_ITEM_NAME","itemName","mainColor","VUE_APP_MAIN_COLOR","components","_Overlay","created","$route","query","lang","handleSetLang","window","plus","plusReady","document","addEventListener","link","head","querySelector","href","require","title","sessionStorage","removeItem","localStorage","getItem","setItem","setInterval","token","getMaxBuyNum","getCustomerService","methods","screen","lockOrientation","mall_max_goods_number_in_order","code","customer_service_url","$set","$i18n","locale","setLanguage","watch","$store.state","newVal","qiangdan","qiangdan_status","to","from","body","scrollTop","documentElement","setTimeout","scrollTo","top","behavior","beforeDestroy","clearInterval"],"sources":["src/App.vue"],"sourcesContent":["<template>\r\n    <div id=\"app\" style=\"width: 100%; height: 100%\">\r\n        <transition :name=\"$router.customRouterData.transitionName\">\r\n            <router-view v-slot=\"{ Component }\">\r\n                <keep-alive :include=\"keepaliveData\">\r\n                    <component :is=\"Component\" />\r\n                </keep-alive>\r\n                <component :is=\"Component\" />\r\n            </router-view>\r\n        </transition>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { Overlay } from \"vant\";\r\nimport { mapMutations, mapGetters } from \"vuex\";\r\nimport { apiHeartbeat, apiGetMaxBuyNum, apiGetCustomerService } from '@/API/common'\r\nimport { apiApplyLoan } from \"./API/loan\";\r\nexport default {\r\n    name: \"App\",\r\n    data() {\r\n        return {\r\n            show: false,\r\n            timer: \"\",\r\n            itemData: this.$store.state.multiItem[process.env.VUE_APP_ITEM_NAME],\r\n            itemName: process.env.VUE_APP_ITEM_NAME,\r\n            mainColor: process.env.VUE_APP_MAIN_COLOR,\r\n        };\r\n    },\r\n    components: {\r\n        [Overlay.name]: Overlay,\r\n    },\r\n    created() {\r\n\r\n        if (this.$route.query.lang) {\r\n            if (this.$route.query.lang == \"cn\") {\r\n                this.handleSetLang(\"cn\");\r\n            } else {\r\n                this.handleSetLang(\"en\");\r\n            }\r\n        }\r\n        if (window.plus) {\r\n            this.plusReady();\r\n        } else {\r\n            document.addEventListener(\"plusready\", this.plusReady, false);\r\n        }\r\n        let link = document.head.querySelector(\"link\");\r\n        link.href = require(`@/assets/${this.itemName}/favicon.png`);\r\n        document.title = this.itemName\r\n        // 刷新事件\r\n        window.addEventListener(\"beforeunload\", () => {\r\n            sessionStorage.removeItem(\"scrollTopCount\")\r\n            // sessionStorage.removeItem(\"showLoading\")\r\n        })\r\n        if (localStorage.getItem('lang') == 'en-US') localStorage.setItem('lang', 'en')\r\n        setInterval(() => {\r\n            let token = localStorage.getItem(\"token\");\r\n            token && apiHeartbeat()\r\n        }, 1000 * 60 * 5);\r\n        this.getMaxBuyNum()\r\n        this.getCustomerService()\r\n    },\r\n\r\n    methods: {\r\n        plusReady() {\r\n            // H5 plus事件处理\r\n            // eslint-disable-next-line no-undef\r\n            plus.screen.lockOrientation(\"portrait-primary\");\r\n\r\n            // 设置系统状态栏背景为红色\r\n            // eslint-disable-next-line no-undef\r\n            // var type = plus.os.name;\r\n            // if (type === \"Android\" || type === \"iOS\") {\r\n            //     switch (this.$route.path) {\r\n            //         case \"/me\":\r\n            //             // eslint-disable-next-line no-undef\r\n            //             plus.navigator.setStatusBarStyle(\"light\");\r\n            //             // eslint-disable-next-line no-undef\r\n            //             plus.navigator.setStatusBarBackground(this.mainColor);\r\n            //             break;\r\n            //         default:\r\n            //             // eslint-disable-next-line no-undef\r\n            //             plus.navigator.setStatusBarStyle(\"dark\");\r\n            //             // eslint-disable-next-line no-undef\r\n            //             plus.navigator.setStatusBarBackground(\"#ffffff\");\r\n            //     }\r\n            // }\r\n\r\n        },\r\n        // 获取商品最大购买数\r\n        async getMaxBuyNum() {\r\n            let { mall_max_goods_number_in_order } = await apiGetMaxBuyNum({ code: 'mall_max_goods_number_in_order' })\r\n            localStorage.setItem('maxBuyNum', mall_max_goods_number_in_order)\r\n        },\r\n        // 获取客服地址\r\n        async getCustomerService() {\r\n            let { customer_service_url } = await apiGetCustomerService('?code=customer_service_url');\r\n\r\n            if (customer_service_url) {\r\n                const { multiItem } = this.$store.state;\r\n                const itemName = this.itemName;\r\n\r\n                // 如果对应的 item 不存在，先初始化为一个对象\r\n                if (!multiItem[itemName]) {\r\n                    // ⚠️ 直接修改 Vuex 的 state 是不推荐的，这里只是防止报错\r\n                    // 正确做法见下方 Vuex 建议\r\n                    this.$set(multiItem, itemName, {});\r\n                }\r\n\r\n                // multiItem[itemName].customer_service = customer_service_url;\r\n            }\r\n        }\r\n        ,\r\n        handleSetLang(lang) {\r\n            // 设置i18n.locale 组件库会按照上面的配置使用对应的文案文件\r\n            this.$i18n.locale = lang;\r\n            // 提交mutations\r\n            this.setLanguage(lang);\r\n        },\r\n\r\n    },\r\n    watch: {\r\n        \"$store.state\": function (newVal) {\r\n            if ($store.state.qiangdan.qiangdan_status) {\r\n                this.show = true;\r\n            }\r\n        },\r\n        $route: function (to, from) {\r\n            if (to.name !== 'home') {\r\n                document.body.scrollTop = 0;\r\n                document.documentElement.scrollTop = 0;\r\n            } else {\r\n                setTimeout(() => {\r\n                    window.scrollTo({\r\n                        top: sessionStorage.getItem(\"scrollTopCount\"),\r\n                        behavior: \"smooth\"\r\n                    });\r\n                }, 500);\r\n            }\r\n        },\r\n    },\r\n    beforeDestroy() {\r\n        clearInterval(this.timer);\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import \"@/assets/init.scss\";\r\n\r\n//返回\r\n.van-nav-bar .van-icon {\r\n    color: #333333;\r\n}\r\n\r\n//// 上传图片框大小设置\r\n// .van-uploader__upload {\r\n//    width:120px !important;\r\n//    height:120px !important;\r\n//}\r\n\r\n.van-uploader__upload-icon {\r\n    font-size: 48px !important;\r\n}\r\n\r\n.van-uploader__upload {\r\n    background-color: white !important;\r\n    border: 2px dashed #b8bcc5;\r\n}\r\n\r\n// tab标签短横线颜色\r\n.van-tabs__line {\r\n    background-color: #1552f0 !important;\r\n}\r\n\r\n//.van-tabs__wrap{\r\n//  height:88px !important;\r\n//}\r\n//\r\n//.van-tab__text--ellipsis{\r\n//   overflow:visible !important;\r\n//}\r\n\r\n.van-tab__text {\r\n    font-size: 14px !important;\r\n}\r\n\r\n.van-nav-bar__text {\r\n    color: #999999;\r\n}\r\n</style>\r\n<style lang=\"scss\" scoped>\r\n// .wrapper {\r\n//   //display: flex;\r\n//   //align-items: center;\r\n//   //justify-content: center;\r\n//   //height: 100%;\r\n//   //width: 100%;\r\n// }\r\n\r\n.block {\r\n    width: 120px;\r\n    height: 120px;\r\n    background-color: #fff;\r\n}\r\n\r\n.zhengti {\r\n    width: 100%;\r\n    max-width: 570px;\r\n    padding: 0 15px;\r\n    //padding-top: 60px;\r\n    box-sizing: border-bo