{"ast":null,"code":"import \"vant/es/toast/style\";\nimport _Toast from \"vant/es/toast\";\nimport \"vant/es/action-sheet/style\";\nimport _ActionSheet from \"vant/es/action-sheet\";\nimport \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'RemindDialog',\n  components: {\n    [_ActionSheet.name]: _ActionSheet,\n    [_Toast.name]: _Toast\n  },\n  props: {\n    dialogShow: {\n      type: Boolean,\n      required: true,\n      default: false\n    },\n    dialogData: {\n      type: Object,\n      required: true,\n      default: () => {}\n    }\n  },\n  computed: {\n    visible: {\n      get() {\n        return this.dialogShow;\n      },\n      set() {\n        this.$emit('update:dialogShow', false);\n      }\n    }\n  },\n  methods: {\n    submitHandle() {\n      _Toast.loading({\n        duration: 0,\n        message: this.$t('提交中'),\n        forbidClick: true\n      });\n      const data = localStorage.getItem('remindStore') ? JSON.parse(localStorage.getItem('remindStore')) : [];\n      if (!data.includes(this.dialogData.id)) {\n        data.push(this.dialogData.id);\n      }\n      localStorage.setItem('remindStore', JSON.stringify(data));\n      setTimeout(() => {\n        _Toast.clear();\n        this.$emit('update:dialogShow', false);\n        this.$emit('done', false);\n      }, 1000);\n    }\n  }\n};","map":{"version":3,"names":["name","components","_ActionSheet","_Toast","props","dialogShow","type","Boolean","required","default","dialogData","Object","computed","visible","get","set","$emit","methods","submitHandle","loading","duration","message","$t","forbidClick","data","localStorage","getItem","JSON","parse","includes","id","push","setItem","stringify","setTimeout","clear"],"sources":["src/page/Order/components/RemindsDialog.vue"],"sourcesContent":["<template>\r\n  <div class=\"dialog-content\">\r\n    <van-action-sheet v-model=\"visible\" :title=\"$t('提醒发货')\" :round=\"false\">\r\n      <div class=\"remind-content\">\r\n        <p>{{ $t('提醒发