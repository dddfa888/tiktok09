cesContent":["<template>\r\n    <div class=\"Successful_Operation\" style=\"min-height: 100vh;\">\r\n        <van-nav-bar safe-area-inset-top :title=\"$t('支付成功')\" fixed class=\"nav-bar\" left-arrow @click-left=\"onClickLeft\">\r\n        </van-nav-bar>\r\n        <div class=\"wanc\">\r\n            <img src=\"@/assets/image/index/wanc.png\" />\r\n        </div>\r\n        <div class=\"title\">{{ $t(\"支付成功\") }}</div>\r\n        <div class=\"text\">\r\n            {{ $t(\"接收者\") }}：{{ (orderInfo.contacts || '') + \" \" + (orderInfo.phone || '') }}\r\n        </div>\r\n        <div class=\"text mt10\">{{ $t(\"收件地址\") }}：{{ orderInfo.address }}</div>\r\n        <div class=\"text mt20\">\r\n            {{ $t(\"收货地址错误？现在联系\") }}\r\n            <span @click=\"goToService\">{{ $t(\"客服\") }}</span>\r\n        </div>\r\n        <div class=\"but but-one\" @click=\"goToHome\">{{ $t(\"返回首页\") }}</div>\r\n        <div class=\"but but-two mt20\" @click=\"goToOrder\">{{ $t(\"检查订单\") }}</div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { NavBar } from \"vant\";\r\nimport { orderInfoPay } from \"@/API/commodity\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            orderInfo: {},\r\n        };\r\n    },\r\n    components: {\r\n        [NavBar.name]: NavBar,\r\n    },\r\n    mounted() {\r\n        this.init();\r\n    },\r\n    created() { },\r\n    methods: {\r\n        init() {\r\n            let id = this.$route.query.orderId;\r\n            if (id) {\r\n                id = id.split(\",\");\r\n            }\r\n            let data = {\r\n                orderId: id[0],\r\n                ades: 0,\r\n                type: 1\r\n            };\r\n            orderInfoPay(data).then((res) => {\r\n                this.orderInfo = res.orderInfo;\r\n                // if(this.orderInfo.phone){\r\n                //   this.orderInfo.phone=this.orderInfo.phone.substring(0,3)+'******'+this.orderInfo.phone.substring(this.orderInfo.phone.length-3)\r\n                // }\r\n            });\r\n        },\r\n        onClickLeft() {\r\n            this.$router.push(this.$route.query?.isCart == 1 ? \"/cart\" : \"-1\");\r\n        },\r\n        goToHome() {\r\n            this.$router.push(\"/\");\r\n        },\r\n        goToOrder() {\r\n            this.$router.push(\"/order\");\r\n        },\r\n        goToService() {\r\n            const path = this.$store.state.multiItem[process.env.VUE_APP_ITEM_NAME].customer_service\r\n            if (path) {\r\n                \r\n                if(window.plus) {\r\n                    plus.runtime.openURL(path);\r\n                } else {\r\n                    window.open(path)\r\n                }\r\n            } else {\r\n                this.$router.push({ path: \"customerServiceIndex?path=customerService\" });\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.Successful_Operation {\r\n    font-size: 14px;\r\n    background: #fff;\r\n    padding-top: 46px;\r\n    color: #333333;\r\n\r\n    .wanc {\r\n        text-align: center;\r\n        padding-top: 70px;\r\n\r\n        img {\r\n            width: 52px;\r\n            height: 52px;\r\n        }\r\n    }\r\n\r\n    .title {\r\n        text-align: center;\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n        padding: 30px 0;\r\n    }\r\n\r\n    .text {\r\n        text-align: center;\r\n        padding: 0 10px;\r\n        font-size: 12px;\r\n\r\n        span {\r\n            \r\n            color: var(--main-color);\r\n        }\r\n    }\r\n\r\n    .but {\r\n        margin: 0 10px;\r\n        height: 45px;\r\n        text-align: center;\r\n        line-height: 45px;\r\n        border-radius: 4px;\r\n        font-size: 16px;\r\n    }\r\n\r\n    .but-one {\r\n        border: 1px solid #333333;\r\n        margin-top: 50px;\r\n    }\r\n\r\n    .but-two {\r\n        background: var(--main-color);\r\n        color: #fff;\r\n    }\r\n\r\n    .mt10 {\r\n        margin-top: 10px;\r\n    }\r\n\r\n    .mt20 {\r\n        margin-top: 20px;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";;;AAsBA,SAAAA,YAAA;AACA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,UAAA;IACA,CAAAC,OAAA,CAAAC,IAAA,GAAAD;EACA;EACAE,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,QAAA;EACAC,OAAA;IACAF,KAAA;MACA,IAAAG,EAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,OAAA;MACA,IAAAH,EAAA;QACAA,EAAA,GAAAA,EAAA,CAAAI,KAAA;MACA;MACA,IAAAb,IAAA;QACAY,OAAA,EAAAH,EAAA;QACAK,IAAA;QACAC,IAAA;MACA;MACAhB,YAAA,CAAAC,IAAA,EAAAgB,IAAA,CAAAC,GAAA;QACA,KAAAhB,SAAA,GAAAgB,GAAA,CAAAhB,SAAA;QACA;QACA;QACA;MACA;IACA;IACAiB,YAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,MAAAV,MAAA,CAAAC,KAAA,EAAAU,MAAA;IACA;IACAC,SAAA;MACA,KAAAH,OAAA,CAAAC,IAAA;IACA;IACAG,UAAA;MACA,KAAAJ,OAAA,CAAAC,IAAA;IACA;IACAI,YAAA;MACA,MAAAC,IAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,SAAA,CAAAC,OAAA,CAAAC,GAAA,CAAAC,iBAAA,EAAAC,gBAAA;MACA,IAAAP,IAAA;QAEA,IAAAQ,MAAA,CAAAC,IAAA;UACAA,IAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAX,IAAA;QACA;UACAQ,MAAA,CAAAI,IAAA,CAAAZ,IAAA;QACA;MACA;QACA,KAAAN,OAAA,CAAAC,IAAA;UAAAK,IAAA;QAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}