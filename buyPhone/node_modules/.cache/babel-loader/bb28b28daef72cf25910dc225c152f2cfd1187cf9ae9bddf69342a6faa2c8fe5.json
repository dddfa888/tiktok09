{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { getLogisticsInformation } from '@/API/order';\nimport { isLogin } from '@/utils/utis';\nexport default {\n  data() {\n    return {\n      list: []\n    };\n  },\n  mounted() {\n    const token = isLogin();\n    const {\n      id\n    } = this.$route.query;\n    getLogisticsInformation({\n      token,\n      orderId: id\n    }).then(res => {\n      this.list = res.map(item => {\n        const arr = item.log.split(id);\n        return {\n          ...item,\n          log: `${this.$t('订单')}<span class=\"log_id\">#${id}</span>${this.$t(arr[1] === '系统发货成功' ? '供应商发货成功' : arr[1])}`\n        };\n      }).reverse();\n    });\n  },\n  methods: {\n    formatTime(date) {\n      date = new Date(date);\n      let Y = date.getFullYear();\n      let M = String(date.getMonth() + 1).padStart(2, '0');\n      let D = String(date.getDate()).padStart(2, '0');\n      let h = String(date.getHours()).padStart(2, '0');\n      let m = String(date.getMinutes()).padStart(2, '0');\n      let s = String(date.getSeconds()).padStart(2, '0');\n      return `${M}/${D}/${Y} ${h}:${m}:${s}`;\n    }\n  }\n};","map":{"version":3,"names":["getLogisticsInformation","isLogin","data","list","mounted","token","id","$route","query","orderId","then","res","map","item","arr","log","split","$t","reverse","methods","formatTime","date","Date","Y","getFullYear","M","String","getMonth","padStart","D","getDate","h","getHours","m","getMinutes","s","getSeconds"],"sources":["src/page/Order/Logistics/index.vue"],"sourcesContent":["<template>\r\n    <div class=\"logistics\">\r\n        <van-nav-bar safe-area-inset-top fixed :title=\"$t('查看物流')\" left-arrow @click-left=\"() => $router.back()\" />\r\n        <van-steps direction=\"vertical\" :active=\"0\">\r\n            <van-step v-for=\"(item, index) of list\" :key=\"index\">\r\n                <p class=\"log\" v-html=\"item.log\"></p>\r\n                <p>{{ $formatZoneDate(item.updateTime) }}</p>\r\n            </van-step>\r\n        </van-steps>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { getLogisticsInformation } from '@/API/order'\r\nimport { isLogin } from '@/utils/utis'\r\nexport default {\r\n    data() {\r\n        return {\r\n            list: []\r\n        }\r\n    },\r\n    mounted() {\r\n        const token = isLogin()\r\n        const { id } = this.$route.query\r\n        getLogisticsInformation({\r\n            token,\r\n            orderId: id\r\n        }).then(res => {\r\n            this.list = res.map(item => {\r\n                const arr = item.log.split(id)\r\n                return {\r\n                    ...item,\r\n                    log: `${this.$t('订单')}<span class=\"log_id\">#${id}</span>${this.$t(arr[1] === '系统发货成功' ? '供应商发货成功' : arr[1])}`\r\n                }\r\n            }).reverse()\r\n        })\r\n    },\r\n    methods: {\r\n        formatTime(date) {\r\n            date = new Date(date);\r\n            let Y = date.getFullYear();\r\n            let M = String(date.getMonth() + 1).padStart(2, '0');\r\n            let D = String(date.getDate()).padStart(2, '0');\r\n            let h = String(date.getHours()).padStart(2, '0');\r\n            let m = String(date.getMinutes()).padStart(2, '0');\r\n            let s = String(date.getSeconds()).padStart(2, '0');\r\n            return `${M}/${D}/${Y} ${h}:${m}:${s}`\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.logistics {\r\n    padding-top: 50px;\r\n\r\n    // font-size: 16px;\r\n    // min-height: 100vh;\r\n    // background-color: #f5f5f7;\r\n    :deep(.van-step--process) {\r\n        p {\r\n            \r\n            color: var(--main-color);\r\n        }\r\n    }\r\n\r\n    .log {\r\n        \r\n        margin-bottom: 2px;\r\n    }\r\n\r\n    .time {\r\n        \r\n        color: var(--main-color);\r\n    }\r\n}\r\n\r\n:deep(.log_id) {\r\n    color: #306ada !important;\r\n}</style>\r\n"],"mappings":";;AAaA,SAAAA,uBAAA;AACA,SAAAC,OAAA;AACA;EACAC,KAAA;IACA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,MAAAC,KAAA,GAAAJ,OAAA;IACA;MAAAK;IAAA,SAAAC,MAAA,CAAAC,KAAA;IACAR,uBAAA;MACAK,KAAA;MACAI,OAAA,EAAAH;IACA,GAAAI,IAAA,CAAAC,GAAA;MACA,KAAAR,IAAA,GAAAQ,GAAA,CAAAC,GAAA,CAAAC,IAAA;QACA,MAAAC,GAAA,GAAAD,IAAA,CAAAE,GAAA,CAAAC,KAAA,CAAAV,EAAA;QACA;UACA,GAAAO,IAAA;UACAE,GAAA,UAAAE,EAAA,+BAAAX,EAAA,eAAAW,EAAA,CAAAH,GAAA,+BAAAA,GAAA;QACA;MACA,GAAAI,OAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAAC,IAAA;MACAA,IAAA,OAAAC,IAAA,CAAAD,IAAA;MACA,IAAAE,CAAA,GAAAF,IAAA,CAAAG,WAAA;MACA,IAAAC,CAAA,GAAAC,MAAA,CAAAL,IAAA,CAAAM,QAAA,QAAAC,QAAA;MACA,IAAAC,CAAA,GAAAH,MAAA,CAAAL,IAAA,CAAAS,OAAA,IAAAF,QAAA;MACA,IAAAG,CAAA,GAAAL,MAAA,CAAAL,IAAA,CAAAW,QAAA,IAAAJ,QAAA;MACA,IAAAK,CAAA,GAAAP,MAAA,CAAAL,IAAA,CAAAa,UAAA,IAAAN,QAAA;MACA,IAAAO,CAAA,GAAAT,MAAA,CAAAL,IAAA,CAAAe,UAAA,IAAAR,QAAA;MACA,UAAAH,CAAA,IAAAI,CAAA,IAAAN,CAAA,IAAAQ,CAAA,IAAAE,CAAA,IAAAE,CAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}