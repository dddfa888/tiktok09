{"ast":null,"code":"import \"vant/es/dialog/style\";\nimport _Dialog from \"vant/es/dialog\";\nimport \"vant/es/toast/style\";\nimport _Toast from \"vant/es/toast\";\nimport \"vant/es/nav-bar/style\";\nimport _NavBar from \"vant/es/nav-bar\";\nimport \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport CancelDialog from \"./../components/CancelDialog.vue\";\nimport RemindsDialog from \"./../components/RemindsDialog.vue\";\nimport { priceFormat } from \"@/utils/utis\";\nimport { orderInfo, orderListGoods, orderReceipt } from \"@/API/user\";\nexport default {\n  name: \"OrderDetails\",\n  components: {\n    [_NavBar.name]: _NavBar,\n    [_Toast.name]: _Toast,\n    [_Dialog.name]: _Dialog,\n    CancelDialog,\n    RemindsDialog\n  },\n  data() {\n    return {\n      copyIcon: require(\"@/assets/image/me/fuzhi1.png\"),\n      cancelShow: false,\n      remindShow: false,\n      currentData: null,\n      orderId: null,\n      totalPrice: 0,\n      goodsData: [],\n      timezone: \"\"\n    };\n  },\n  computed: {\n    goodsTotalNum() {\n      let num = 0;\n      this.goodsData.forEach(item => {\n        num += item.goodsNum;\n      });\n      return num;\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n      const {\n        id\n      } = this.$route.query;\n      if (id) {\n        this.orderId = id;\n        this.getDataInfo();\n        this.getOrderGoods();\n      } else {\n        _Toast(this.$t(\"订单号不存在\"));\n        setTimeout(() => {\n          this.$router.back();\n        }, 1500);\n      }\n    });\n    this.timezone = localStorage.getItem(\"timeZone\");\n  },\n  methods: {\n    repeatPurchase(item) {\n      if (item?.isShelf != 1 || item?.isValid != 1) return this.$notify(this.$t('该商品已下架'), 'warning');\n      this.$router.push('/CommodityDetails?sellerGoodsId=' + item.goodsId);\n    },\n    time(t) {\n      let dayjs = this.dayjs;\n      return dayjs().tz(this.timezone).format(t);\n    },\n    priceFormat(num) {\n      return priceFormat(num);\n    },\n    copyHandle(txt) {\n      this.$copyText(txt).then(() => {\n        _Toast(this.$t(\"复制成功\"));\n      }, () => {\n        _Toast(this.$t(\"复制失败\"));\n      });\n    },\n    getDataInfo() {\n      _Toast.loading({\n        duration: 0,\n        // message: this.$t(\"请求中\"),\n        forbidClick: true\n      });\n      orderInfo({\n        orderId: this.orderId,\n        type: 1\n      }).then(res => {\n        console.log(res, \"=====res\");\n        this.currentData = res.orderInfo;\n        _Toast.clear();\n      }).catch(() => {\n        _Toast.clear();\n      });\n    },\n    getOrderGoods() {\n      orderListGoods({\n        orderId: this.orderId\n      }).then(res => {\n        console.log(\"res======\", res);\n        this.goodsData = res.pageList || [];\n        this.goodsData.forEach(item => {\n          this.totalPrice += item.goodsReal * item.goodsNum;\n          if (item.goodType == \"虚拟商品\") {\n            this.currentData.goodType = 'virtual';\n          }\n          // this.$operation(this.totalPrice, this.$operation(item.goodsReal, item.goodsNum, 'plus'), 'times')\n          // this.totalPrice += this.$operation(item.goodsReal, item.goodsNum, 'times');\n        });\n      });\n    },\n    //跳转待支付页面\n    openPeddingPay(data) {\n      const list = [{\n        orderList: data.id\n      }];\n      this.$router.push({\n        path: \"/PendingPayment\",\n        query: {\n          orderList: JSON.stringify(list),\n          total: this.$operation(data.discountPrice, data.goodsCount, 'times') || this.$operation(data.priceReal, data.goodsCount, 'times')\n        }\n      });\n    },\n    // 提醒发货\n    remindHandle(data) {\n      const remindStore = localStorage.getItem(\"remindStore\") ? JSON.parse(localStorage.getItem(\"remindStore\")) : [];\n      if (!remindStore.includes(data.id)) {\n        this.remindShow = true;\n      } else {\n        _Toast(this.$t(\"该订单已提醒过啦~\"));\n      }\n    },\n    openPage(path, query) {\n      const params = {\n        path\n      };\n      if (query) {\n        params.query = query;\n      }\n      this.$router.push(params);\n    },\n    // 收货\n    orderPick(data) {\n      _Dialog.confirm({\n        title: this.$t(\"提示\"),\n        message: this.$t(\"确认收货吗？\"),\n        confirmButtonText: this.$t(\"确认\"),\n        cancelButtonText: this.$t(\"取消\")\n      }).then(() => {\n        _Toast.loading({\n          duration: 0,\n          message: this.$t(\"提交中\"),\n          forbidClick: true\n        });\n        const params = {\n          orderId: data.id\n        };\n        orderReceipt(params).then(() => {\n          _Toast.clear();\n          this.currentData = null;\n          // this.tableChange();\n          this.getDataInfo();\n          // this.getOrderGoods();\n        }).catch(() => {\n          _Toast.clear();\n        });\n      }).catch(() => {\n        console.log(\"cancel\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["CancelDialog","RemindsDialog","priceFormat","orderInfo","orderListGoods","orderReceipt","name","components","_NavBar","_Toast","_Dialog","data","copyIcon","require","cancelShow","remindShow","currentData","orderId","totalPrice","goodsData","timezone","computed","goodsTotalNum","num","forEach","item","goodsNum","mounted","$nextTick","id","$route","query","getDataInfo","getOrderGoods","$t","setTimeout","$router","back","localStorage","getItem","methods","repeatPurchase","isShelf","isValid","$notify","push","goodsId","time","t","dayjs","tz","format","copyHandle","txt","$copyText","then","loading","duration","forbidClick","type","res","console","log","clear","catch","pageList","goodsReal","goodType","openPeddingPay","list","orderList","path","JSON","stringify","total","$operation","discountPrice","goodsCount","priceReal","remindHandle","remindStore","parse","includes","openPage","params","orderPick","confirm","title","message","confirmButtonText","cancelButtonText"],"sources":["src/page/Order/Details/index.vue"],"sourcesContent":["<template>\r\n    <div class=\"page-content\">\r\n        <!-- 取消弹窗 -->\r\n        <cancel-dialog v-if=\"currentData\" :dialog-show.sync=\"cancelShow\" :dialog-data=\"currentData\"\r\n            @done=\"() => $router.back()\" />\r\n\r\n        <!-- 提醒发货 -->\r\n        <reminds-dialog v-if=\"currentData\" :dialog-show.sync=\"remindShow\" :dialog-data=\"currentData\" />\r\n\r\n        <van-nav-bar safe-area-inset-top fixed :title=\"$t('订单详情')\" left-arrow\r\n            @click-left=\"() => $router.push({ path: '/order', dir: 'left', query: { type: $route.query.index } })\" />\r\n        <div style=\"width: 100%; height: 46px\" />\r\n\r\n        <div v-if=\"currentData\" class=\"info-content\">\r\n            <div class=\"order-info-content\">\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"流水号\") }}：</p>\r\n                    <div>\r\n                        <div class=\"copy\" @click=\"copyHandle(currentData.id)\">\r\n                            <span class=\"nowrap\" style=\"width: 200px\">{{\r\n                                currentData.id\r\n                            }}</span><img :src=\"copyIcon\" alt=\"\" />\r\n                        </div>\r\n                        <p class=\"tips\">{{ $formatZoneDate(currentData.createTime) }}</p>\r\n                    </div>\r\n                </div>\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"收件地址\") }}：</p>\r\n                    <div>\r\n                        <p>\r\n                            {{ currentData.goodType = 'virtual' ? \"--\" : currentData.postcode }} {{ currentData.goodType = 'virtual' ? \"--\" : currentData.province }}\r\n                            {{ currentData.goodType = 'virtual' ? \"--\" : currentData.city }}\r\n                        </p>\r\n                        <p>{{ currentData.goodType = 'virtual' ? \"--\" : currentData.country }}</p>\r\n                        <p>{{ currentData.goodType = 'virtual' ? \"--\" : currentData.address }}</p>\r\n                        <p>{{ currentData.contacts }}</p>\r\n                        <p>\r\n                            {{\r\n                                currentData.phone?.indexOf(\"|\") > -1\r\n                                ? currentData.phone.split(\"|\")[1]\r\n                                : currentData.phone\r\n                            }}\r\n                        </p>\r\n                    </div>\r\n                </div>\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"商品数量\") }}：</p>\r\n                    <div>{{ goodsTotalNum }}</div>\r\n                </div>\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"订单总额\") }}：</p>\r\n                    <div class=\"money\">\r\n                    ${{ priceFormat(totalPrice + currentData.tax + currentData.fees) }}\r\n                        <!-- ${{ priceFormat($plus($plus(totalPrice, currentData.tax), currentData.fees)) }} -->\r\n                    </div>\r\n                </div>\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"税费\") }}：</p>\r\n                    <div class=\"money\">${{ currentData.tax ? priceFormat(currentData.tax) : '0.00' }}</div>\r\n                </div>\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"运费\") }}：</p>\r\n                    <div class=\"money\">${{ currentData.fees ? priceFormat(currentData.fees) : '0.00' }}</div>\r\n                </div>\r\n                <div class=\"item\">\r\n                    <p class=\"p80\">{{ $t(\"总金额\") }}：</p>\r\n                    <div class=\"money\">\r\n                        ${{ priceFormat($plus($plus(totalPrice, currentData.tax), currentData.fees)) }}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n            <div v-if=\"currentData\" class=\"goods-info-content\">\r\n                <div v-for=\"(item, index) in goodsData\" :key=\"index\" class=\"order-info-content\">\r\n                    <div class=\"goods-item\" @click=\"repeatPurchase(item)\">\r\n                        <!-- <div class=\"poster\"><img :src=\"item.goodsIcon\" alt=\"\" /></div> -->\r\n                        <div class=\"poster\">\r\n                            <img :src=\"item.goodsIcon\" alt=\"\" />\r\n                        </div>\r\n                        <div class=\"name\">\r\n                            <div class=\"title\">{{ item.goodsName }}</div>\r\n                            <div class=\"\" v-if=\"item.attributes\">\r\n                                <span class=\"attribute\" v-for=\"(j, p) in item.attributes\" :key=\"p\"\r\n                                    v-if=\"j.attrName && j.attrValue\">\r\n                                    {{ j.attrName }}:{{ j.attrValue }}</span>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <p>{{ $t(\"数量\") }}：</p>\r\n                        <div>{{ item.goodsNum }}</div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <p>{{ $t(\"单价\") }}：</p>\r\n                        <div class=\"money\">${{ priceFormat(item.goodsReal) }}</div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <p>{{ $t(\"总价\") }}：</p>\r\n                        <div class=\"money\">\r\n                            ${{ priceFormat($operation(item.goodsReal, item.goodsNum, 'times')) }}\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <p>{{ $t(\"税费\") }}：</p>\r\n                        <div class=\"money\">${{ item.tax ? priceFormat(item.tax) : '0.00' }}</div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <p>{{ $t(\"运费\") }}：</p>\r\n                        <div class=\"money\">${{ item.fees ? priceFormat(item.fees) : '0.00' }}</div>\r\n                    </div>\r\n                    <div class=\"item\">\r\n                        <p>{{ $t(\"总计\") }}：</p>\r\n                        <div class=\"money\">\r\n                            {{ priceFormat(item.goodsReal * item.goodsNum + item.tax + item.fees) }}\r\n                            <!-- ${{ priceFormat($plus($plus(totalPrice, currentData.tax), currentData.fees)) }} -->\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div v-if=\"currentData && Number(currentData.status) !== -1\" class=\"page-footer\">\r\n            <div v-if=\"Number(currentData.status) === 0\" class=\"btn\" @click=\"cancelShow = true\">\r\n                {{ $t(\"取消订单\") }}\r\n            </div>\r\n            <div v-if=\"Number(currentData.status) === 0\" class=\"btn color\" @click=\"openPeddingPay(currentData)\">\r\n                {{ $t(\"立即付款\") }}\r\n            </div>\r\n            <div v-if=\"Number(currentData.status) === 1\" class=\"btn color\" @click=\"remindHandle(currentData)\">\r\n                {{ $t(\"提醒发货\") }}\r\n            </div>\r\n            <div v-if=\"Number(currentData.status) === 3\" class=\"btn\" @click=\"\r\n                openPage('/orderRefund', {\r\n                    id: currentData.id,\r\n                    a: currentData.prizeReal,\r\n                })\r\n            \">\r\n                {{ $t(\"申请退货\") }}\r\n            </div>\r\n\r\n            <div v-if=\"Number(currentData.status) === 3\" class=\"btn color\" @click=\"orderPick(currentData)\">\r\n                {{ $t(\"确认收货\") }}\r\n            </div>\r\n            <div v-if=\"Number(currentData.status) > 0\" class=\"btn\"\r\n                @click=\"openPage('/order/logistics', { id: currentData.id })\">\r\n                {{ $t(\"查看物流\") }}\r\n            </div>\r\n            <div v-if=\"Number(currentData.status) === 4\" class=\"btn\"\r\n                @click=\"openPage('/orderEvaluate', { id: currentData.id })\">\r\n                {{ $t(\"立即评价\") }}\r\n            </div>\r\n            <div v-if=\"Number(currentData.status) === 6\" class=\"tips\">\r\n                <span v-if=\"!Boolean($route.query.returnStatus)\">{{ $t(\"未退款\") }}\r\n                </span>\r\n                <span v-if=\"Number($route.query.returnStatus) === 1\" class=\"running\">{{\r\n                    $t(\"退款中\")\r\n                }}</span>\r\n                <span v-if=\"Number($route.query.returnStatus) === 2\" class=\"success\">{{\r\n                    $t(\"退款成功\")\r\n                }}</span>\r\n                <span v-if=\"Number($route.query.returnStatus) === 3\" class=\"fail\">{{\r\n                    $t(\"退款失败\")\r\n                }}</span>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { NavBar, Toast, Dialog } from \"vant\";\r\nimport CancelDialog from \"./../components/CancelDialog.vue\";\r\nimport RemindsDialog from \"./../components/RemindsDialog.vue\";\r\nimport { priceFormat } from \"@/utils/utis\";\r\nimport { orderInfo, orderListGoods, orderReceipt } from \"@/API/user\";\r\nexport default {\r\n    name: \"OrderDetails\",\r\n    components: {\r\n        [NavBar.name]: NavBar,\r\n        [Toast.name]: Toast,\r\n        [Dialog.name]: Dialog,\r\n        CancelDialog,\r\n        RemindsDialog,\r\n    },\r\n    data() {\r\n        return {\r\n            copyIcon: require(\"@/assets/image/me/fuzhi1.png\"),\r\n            cancelShow: false,\r\n            remindShow: false,\r\n            currentData: null,\r\n            orderId: null,\r\n            totalPrice: 0,\r\n            goodsData: [],\r\n            timezone: \"\",\r\n        };\r\n    },\r\n    computed: {\r\n        goodsTotalNum() {\r\n            let num = 0;\r\n            this.goodsData.forEach((item) => {\r\n                num += item.goodsNum;\r\n            });\r\n            return num;\r\n        },\r\n    },\r\n    mounted() {\r\n        this.$nextTick(() => {\r\n            const { id } = this.$route.query;\r\n            if (id) {\r\n                this.orderId = id;\r\n                this.getDataInfo();\r\n                this.getOrderGoods();\r\n            } else {\r\n                Toast(this.$t(\"订单号不存在\"));\r\n                setTimeout(() => {\r\n                    this.$router.back();\r\n                }, 1500);\r\n            }\r\n        });\r\n        this.timezone = localStorage.getItem(\"timeZone\");\r\n    },\r\n    methods: {\r\n        repeatPurchase(item) {\r\n            if (item?.isShelf != 1 || item?.isValid != 1) return this.$notify(this.$t('该商品已下架'), 'warning')\r\n            this.$router.push('/CommodityDetails?sellerGoodsId=' + item.goodsId)\r\n        },\r\n        time(t) {\r\n            let dayjs = this.dayjs;\r\n            return dayjs().tz(this.timezone).format(t);\r\n        },\r\n        priceFormat(num) {\r\n            return priceFormat(num);\r\n        },\r\n        copyHandle(txt) {\r\n            this.$copyText(txt).then(\r\n                () => {\r\n                    Toast(this.$t(\"复制成功\"));\r\n                },\r\n                () => {\r\n                    Toast(this.$t(\"复制失败\"));\r\n                }\r\n            );\r\n        },\r\n\r\n        getDataInfo() {\r\n            Toast.loading({\r\n                duration: 0,\r\n                // message: this.$t(\"请求中\"),\r\n                forbidClick: true,\r\n            });\r\n\r\n            orderInfo({\r\n                orderId: this.orderId,\r\n                type: 1\r\n            })\r\n                .then((res) => {\r\n                    console.log(res,\"=====res\");\r\n                    this.currentData = res.orderInfo;\r\n                    Toast.clear();\r\n                })\r\n                .catch(() => {\r\n                    Toast.clear();\r\n                });\r\n        },\r\n\r\n        getOrderGoods() {\r\n            orderListGoods({\r\n                orderId: this.orderId,\r\n            }).then((res) => {\r\n                console.log(\"res======\",res);\r\n                this.goodsData = res.pageList || [];\r\n                this.goodsData.forEach((item) => {\r\n                    this.totalPrice += item.goodsReal * item.goodsNum\r\n                    if (item.goodType == \"虚拟商品\") {\r\n                        this.currentData.goodType = 'virtual'\r\n                    }\r\n                    // this.$operation(this.totalPrice, this.$operation(item.goodsReal, item.goodsNum, 'plus'), 'times')\r\n                    // this.totalPrice += this.$operation(item.goodsReal, item.goodsNum, 'times');\r\n                });\r\n            });\r\n        },\r\n\r\n        //跳转待支付页面\r\n        openPeddingPay(data) {\r\n            const list = [\r\n                {\r\n                    orderList: data.id,\r\n                },\r\n            ];\r\n            this.$router.push({\r\n                path: \"/PendingPayment\",\r\n                query: {\r\n                    orderList: JSON.stringify(list),\r\n                    total: this.$operation(data.discountPrice, data.goodsCount, 'times') ||\r\n                        this.$operation(data.priceReal, data.goodsCount, 'times')\r\n                },\r\n            });\r\n        },\r\n\r\n        // 提醒发货\r\n        remindHandle(data) {\r\n            const remindStore = localStorage.getItem(\"remindStore\")\r\n                ? JSON.parse(localStorage.getItem(\"remindStore\"))\r\n                : [];\r\n            if (!remindStore.includes(data.id)) {\r\n                this.remindShow = true;\r\n            } else {\r\n                Toast(this.$t(\"该订单已提醒过啦~\"));\r\n            }\r\n        },\r\n\r\n        openPage(path, query) {\r\n            const params = {\r\n                path,\r\n            };\r\n\r\n            if (query) {\r\n                params.query = query;\r\n            }\r\n\r\n            this.$router.push(params);\r\n        },\r\n\r\n        // 收货\r\n        orderPick(data) {\r\n            Dialog.confirm({\r\n                title: this.$t(\"提示\"),\r\n                message: this.$t(\"确认收货吗？\"),\r\n                confirmButtonText: this.$t(\"确认\"),\r\n                cancelButtonText: this.$t(\"取消\"),\r\n            })\r\n                .then(() => {\r\n                    Toast.loading({\r\n                        duration: 0,\r\n                        message: this.$t(\"提交中\"),\r\n                        forbidClick: true,\r\n                    });\r\n\r\n                    const params = {\r\n                        orderId: data.id,\r\n                    };\r\n\r\n                    orderReceipt(params)\r\n                        .then(() => {\r\n                            Toast.clear();\r\n                            this.currentData = null\r\n                            // this.tableChange();\r\n                            this.getDataInfo();\r\n                            // this.getOrderGoods();\r\n                        })\r\n                        .catch(() => {\r\n                            Toast.clear();\r\n                        });\r\n                })\r\n                .catch(() => {\r\n                    console.log(\"cancel\");\r\n                });\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.p80 {\r\n    flex: 0 0 70px;\r\n}\r\n\r\n.page-content {\r\n    min-height: 100vh;\r\n    background-color: #f5f5f7;\r\n\r\n    * {\r\n        box-sizing: border-box;\r\n    }\r\n\r\n    .info-content {\r\n        padding: 10px 15px;\r\n        padding-bottom: 100px;\r\n\r\n        .order-info-content {\r\n            padding: 0 10px;\r\n            background-color: #fff;\r\n\r\n            >.item {\r\n                padding: 17px 0;\r\n                border-bottom: 1px solid #eee;\r\n                color: #000;\r\n                font-size: 14px;\r\n                display: flex;\r\n                justify-content: space-between;\r\n\r\n                .p80 {\r\n                    flex: 0 0 100px;\r\n                }\r\n\r\n                >div {\r\n                    flex: 1;\r\n                    // padding-left: 50px;\r\n                    text-align: end;\r\n                    word-break: break-word;\r\n\r\n                    &.money {\r\n                        font-size: 16px;\r\n                        \r\n                        color: var(--main-color);\r\n                    }\r\n\r\n                    >p {\r\n                        line-height: 21px;\r\n\r\n                        &.tips {\r\n                            font-size: 12px;\r\n                            color: #999;\r\n                            margin-top: 6px;\r\n                        }\r\n                    }\r\n\r\n                    >.copy {\r\n                        display: flex;\r\n                        justify-content: flex-end;\r\n\r\n                        >img {\r\n                            width: 10px;\r\n                            height: 10px;\r\n                            margin-left: 8px;\r\n                            position: relative;\r\n                            top: 3px;\r\n                        }\r\n                    }\r\n                }\r\n\r\n                &:last-child {\r\n                    border-bottom: 0;\r\n                }\r\n            }\r\n        }\r\n\r\n        >.goods-info-content {\r\n            margin-top: 16px;\r\n\r\n            >.order-info-content {\r\n                margin-bottom: 6px;\r\n            }\r\n\r\n            .goods-item {\r\n                display: flex;\r\n                padding: 17px 0;\r\n                border-bottom: 1px solid #eee;\r\n\r\n                >.poster {\r\n                    width: 63px;\r\n                    height: 63px;\r\n                    display: flex;\r\n                    align-items: center;\r\n                    justify-content: center;\r\n                    overflow: hidden;\r\n\r\n                    >img {\r\n                        height: fit-content;\r\n                        width: auto;\r\n                        max-height: 100%;\r\n                        max-width: 100%;\r\n                    }\r\n                }\r\n\r\n                >.name {\r\n                    word-break: break-word;\r\n                    flex: 1;\r\n                    font-size: 14px;\r\n                    padding-left: 20px;\r\n\r\n                    .title {\r\n                        display: -webkit-box;\r\n                        -webkit-box-orient: vertical;\r\n                        -moz-box-orient: vertical;\r\n                        box-orient: vertical;\r\n                        -webkit-line-clamp: 2;\r\n                        line-clamp: 2;\r\n                        overflow: hidden;\r\n                        text-overflow: ellipsis;\r\n                    }\r\n\r\n                    .attribute {\r\n                        font-size: 12px;\r\n                        color: #333;\r\n                        margin-right: 10px;\r\n                        margin-top: 5px;\r\n                        display: inline-block;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    .page-footer {\r\n        width: 100%;\r\n        height: 53px;\r\n        background-color: #fff;\r\n        position: fixed;\r\n        bottom: 0;\r\n        left: 0;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: flex-end;\r\n        padding-right: 15px;\r\n\r\n        >.btn {\r\n            min-width: 97px;\r\n            height: 32px;\r\n            padding: 0 10px;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: 